<div id="<%= dom_id program %>">

  <div class="grid grid-cols-1 md:grid-cols-4 gap-4 "> 
    <p>
      <strong>Program stage:</strong><br>
      <%= program.program_stage %>
    </p>

    <p>
      <strong>Program name:</strong><br>
      <%= program.program_name %>
    </p>

    <p>
     <strong>Na program contact email:</strong><br>
       <%= program.na_program_contact_email %>
    </p>

    <p>
      <strong>Na program contact phone:</strong><br>
      <%= program.na_program_contact_phone %>
    </p>

    <p>
    </p>

    <p>
      <strong>Location name:</strong><br>
      <%= program.location_name %>
    </p>

    <p>
      <strong>Location contact phone:</strong><br>
      <%= program.location_contact_phone %>
    </p>

    <p>
      <strong>Location contact email:</strong><br>
      <%= program.location_contact_email %>
    </p>
   
    <p>
      <strong>Address:</strong><br>
      <%= program.address %> 
    </p>

    <p>
      <strong>City:</strong><br>
      <%= program.city %>
    </p>

    <div class="flex justify-between">
      <p>
        <strong>State:</strong><br>
        <%= program.state %>
      </p>

      <p>
        <strong>Zip:</strong><br>
        <%= program.zip %>
      </p>
    </div>

    <p>
     <strong>Country:</strong><br>
       <%= program.country %>
    </p>
    
    <p>
    <strong>Coach Assigned:</strong><br>
    <%= Person.where(id: program.people_id).first.full_name unless program.people_id.to_s.empty?  %>
  </p>
   
    <p>
      <strong>Program Event Date / time:</strong><br>
      <%= program.program_event_datetime.to_formatted_s(:usa_dt) unless program.program_event_datetime.to_s.empty? %>
    </p>

    <p>
      <strong>Timezone:</strong><br>
      <%= program.timezone %>
    </p>

    <p>
     <strong>Funded by:</strong><br>
      <%= program.funded_by %>
    </p>

    <p class="col-span-3">
      <strong>Notes:</strong><br>
      <%= program.notes %>
    </p>

    </div>
</div>
